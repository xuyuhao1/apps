            android:textSize="16sp"
            android:textColor="#ff333333"
            android:layout_marginTop="39dp"
            android:layout_width="100dp"
            android:layout_height="34dp"
            />

        <CheckBox
            android:id="@+id/jbLabel3"
            style="@style/select_style"
            android:tag="rd2"
            android:background="@drawable/btn_rd2"
            android:text="伪装大师"
            android:textSize="16sp"
            android:textColor="#ff333333"
            android:layout_marginTop="39dp"
            android:layout_width="100dp"
            android:layout_height="34dp"
            />

        <CheckBox
            android:id="@+id/jbLabel4"
            style="@style/select_style"
            android:tag="rd2"
            android:background="@drawable/btn_rd2"
            android:text="戏精"
            android:textSize="16sp"
            android:textColor="#ff333333"
            android:layout_width="100dp"
            android:layout_height="34dp"
            android:layout_marginTop="86dp"
            android:layout_marginLeft="-320dp"
            />

        <CheckBox
            android:id="@+id/jbLabel5"
            style="@style/select_style"
            android:tag="rd2"
            android:background="@drawable/btn_rd2"
            android:text="修仙者"
            android:textSize="16sp"
            android:textColor="#ff333333"
            android:layout_width="100dp"
            android:layout_height="34dp"
            android:layout_marginTop="86dp"
            android:layout_marginLeft="5dp"
            />

        <CheckBox
            android:id="@+id/jbLabel6"
            style="@style/select_style"
            android:tag="rd2"
            android:background="@drawable/btn_rd2"
            android:text="读本小白"
            android:textSize="16sp"
            android:textColor="#ff333333"
            android:layout_width="100dp"
            android:layout_height="34dp"
            android:layout_marginTop="86dp"
            android:layout_marginLeft="5dp"
            />
    </LinearLayout>
</RelativeLayout>