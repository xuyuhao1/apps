<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="27dp"
        android:layout_marginTop="10dp">
        <ImageButton
            android:id="@+id/back"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:background="@mipmap/back"
            android:layout_marginLeft="17dp"></ImageButton>

        <TextView
            android:layout_width="72dp"
            android:layout_height="25dp"
            android:layout_marginLeft="127dp"
            android:text="标签选择"
            android:textColor="#ff323232"
            android:textSize="18sp" />

        <Button
            android:id="@+id/login2Btn"
            android:layout_width="50dp"
            android:layout_height="27dp"
            android:layout_marginLeft="81dp"
            android:background="@drawable/btn_normal"
            android:text="保存"
            android:textColor="#000000"
            android:textSize="12sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"></Button>
    </LinearLayout>
    <LinearLayout
        android:layout_width="328dp"
        android:layout_height="122dp"
        android:layout_marginTop="83dp"
        android:layout_marginLeft="24dp">
        <TextView
            android:layout_width="90dp"
            android:layout_height="21dp"
            android:text="你的密室标签"
            android:textColor="#ff333333"
            android:textSize="15sp"
            />
        <CheckBox
            android:id="@+id/msLabel1"
            style="@style/select_style"
            android:tag="rd1"
            android:background="@drawable/btn_rd2"
            android:text="游荡NPC"
            android:textColor="#ff333333"
            android:textSize="16sp"
            android:layout_width="100dp"
            android:layout_height="34dp"
            android:layout_marginTop="39dp"
            android:layout_marginLeft="-90dp"
            />

        <CheckBox
            android:id="@+id/msLabel2"
            style="@style/select_style"
            android:tag="rd2"
            android:background="@drawable/btn_rd2"
            android:text="气氛尖叫鸡"
            android:textSize="16sp"
            android:textColor="#ff333333"
            android:layout_marginTop="39dp"
            android:layout_width="100dp"
            android:layout_height="34dp"
            />

        <CheckBox
            android:id="@+id/msLabel3"
            style="@style/select_style"
            android:tag="rd2"
            android:background="@drawable/btn_rd2"
            android:text="铁胆无畏"
            android:textSize="16sp"
            android:textColor="#ff333333"
            android:layout_marginTop="39dp"
            android:layout_width="100dp"
            android:layout_height="34dp"
            />

        <CheckBox
            android:id="@+id/msLabel4"
            style="@style/select_style"
            android:tag="rd2"
            android:background="@drawable/btn_rd2"
            android:text="揭秘大师"
            android:textSize="16sp"
            android:textColor="#ff333333"
            android:layout_width="100dp"
            android:layout_height="34dp"
            android:layout_marginTop="86dp"
            android:layout_marginLeft="-320dp"
            />

        <CheckBox
            android:id="@+id/msLabel5"
            style="@style/select_style"
            android:tag="rd2"
            android:background="@drawable/btn_rd2"
            android:text="走喂奶妈"
            android:textSize="16sp"
            android:textColor="#ff333333"
            android:layout_width="100dp"
            android:layout_height="34dp"
            android:layout_marginTop="86dp"
            android:layout_marginLeft="5dp"
            />

        <CheckBox
            android:id="@+id/msLabel6"
            style="@style/select_style"
            android:tag="rd2"
            android:background="@drawable/btn_rd2"
            android:text="好事村名"
            android:textSize="16sp"
            android:textColor="#ff333333"
            android:layout_width="100dp"
            android:layout_height="34dp"
            android:layout_marginTop="86dp"
            android:layout_marginLeft="5dp"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="328dp"
        android:layout_height="122dp"
        android:layout_marginTop="259dp"
        android:layout_marginLeft="24dp">
        <TextView
            android:layout_width="90dp"
            android:layout_height="21dp"
            android:text="你的剧本标签"
            android:textColor="#ff333333"
            android:textSize="15sp"
            />
        <CheckBox
            android:id="@+id/jbLabel1"
            style="@style/select_style"
            android:tag="rd1"
            android:background="@drawable/btn_rd2"
            android:text="无情推土机"
            android:textColor="#ff333333"
            android:textSize="16sp"
            android:layout_width="100dp"
            android:layout_height="34dp"
            android:layout_marginTop="39dp"
            android:layout_marginLeft="-90dp"
            />

        <CheckBox
            android:id="@+id/jbLabel2"
            style="@style/select_style"
            android:tag="rd2"
            android:background="@drawable/btn_rd2"
